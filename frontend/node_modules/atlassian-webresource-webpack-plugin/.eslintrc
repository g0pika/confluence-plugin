{
    "extends": [
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jasmine": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "es6": true
      }
    },
    "plugins": [
      "filenames",
      "node",
      "prettier",
      "jasmine"
    ],
    "rules": {
      "camelcase": 1,
      "consistent-return": 0,
      "curly": 2,
      "dot-notation": 1,
      "eol-last": 0,
      "eqeqeq": 2,
      "filenames/match-exported": 2,
      "handle-callback-err": 0,
      "new-cap": 2,
      "no-alert": 1,
      "no-cond-assign": 1,
      "no-console": 2,
      "no-debugger": 2,
      "no-dupe-class-members": 2,
      "no-dupe-keys": 2,
      "no-else-return": 2,
      "no-empty": 2,
      "no-extend-native": 1,
      "no-extra-boolean-cast": 2,
      "no-inner-declarations": 1,
      "no-loop-func": 1,
      "no-mixed-requires": 0,
      "no-multi-spaces": [2, { "ignoreEOLComments": true }],
      "no-new": 1,
      "no-redeclare": 2,
      "no-regex-spaces": 2,
      "no-return-assign": 0,
      "no-script-url": 2,
      "no-shadow": 2,
      "no-spaced-func": 2,
      "no-undef": 2,
      "no-underscore-dangle": 0,
      "no-unreachable": 2,
      "no-unused-expressions": 2,
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "none"
        }
      ],
      "no-use-before-define": 2,
      "node/exports-style": [2, "module.exports", { "allowBatchAssign": false }],
      "space-infix-ops": 2,
      "space-unary-ops": 2,
      "strict": 2,
      "prettier/prettier": 2
    }
  }
